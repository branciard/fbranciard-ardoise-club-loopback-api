{
  "name": "Address",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "city": {
      "type": "string",
      "required": false
    },
    "geo": {
      "type": "geopoint",
      "required": false
    },
    "state": {
      "type": "string",
      "required": false
    },
    "street": {
      "type": "string",
      "required": false
    },
    "zipcode": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "shop": {
      "type": "belongsTo",
      "model": "Shop",
      "foreignKey": "shopId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
