{
  "name": "Shop",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "category": {
      "type": "string",
      "required": true
    },
    "clientsNumber": {
      "type": "number",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "pepsShop": {
      "type": "number",
      "required": true
    },
    "tel": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "profile": {
      "type": "belongsTo",
      "model": "Profile",
      "foreignKey": "fk_profile_id"
    },
    "address": {
      "type": "hasOne",
      "model": "Address",
      "foreignKey": "fk_address_id"
    },
    "dailyboard": {
      "type": "hasOne",
      "model": "DailyBoard",
      "foreignKey": "fk_dailyboard_id"
    },
    "nowasteboard": {
      "type": "hasOne",
      "model": "NoWasteBoard",
      "foreignKey": "fk_nowasteboard_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
