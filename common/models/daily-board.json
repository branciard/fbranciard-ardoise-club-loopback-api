{
  "name": "DailyBoard",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "dailyitems": {
      "type": [
        "object"
      ],
      "required": false
    },
    "title": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "shop": {
      "type": "belongsTo",
      "model": "Shop",
      "foreignKey": "fk_shop_id"
    },
    "dailyboardsubscription": {
      "type": "hasMany",
      "model": "Profile",
      "foreignKey": "fk_profile_id",
      "through": "DailyBoardSubscription"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
